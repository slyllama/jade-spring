config_version=5

[application]

config/name="Jade Spring"
run/main_scene="res://lib/main_menu/main_menu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="jade-spring"
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=60
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://generic/textures/splash.png"
config/icon="res://icon.png"
boot_splash/minimum_display_time=1500

[autoload]

Global="*res://lib/_global/global.tscn"
Utilities="*res://lib/_global/utilities.gd"
SettingsHandler="*res://lib/settings_handler/settings_handler.tscn"
Save="*res://lib/_global/save.gd"

[debug]

gdscript/warnings/unused_signal=0
shapes/collision/shape_color=Color(0, 0.72, 0.8, 1)

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
mouse_cursor/custom_image="res://generic/textures/cursor.png"

[editor]

movie_writer/movie_file="D:/Repos/seitung-spring-clean/build/out.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/mmo_camera/plugin.cfg")

[filesystem]

import/blender/enabled=false

[gui]

timers/tooltip_delay_sec=0.07
theme/custom="res://generic/themes/dwelt.tres"
theme/custom_font="res://generic/fonts/libertinus_sans.otf"

